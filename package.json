{
  "name": "webcut",
  "version": "0.2.1",
  "description": "A powerful video editing canvas UI library for web applications",
  "main": "esm/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "webcomponents": "webcomponents/index.js",
  "files": [
    "esm",
    "webcomponents",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tangshuang/webcut.git"
  },
  "bugs": {
    "url": "https://github.com/tangshuang/webcut/issues"
  },
  "homepage": "https://webcut.top",
  "scripts": {
    "docs": "docsify serve docs",
    "dev": "vite",
    "build": "npm run build:esm && npm run build:webcomponents && npm run build:webcomponents_bundle",
    "build:esm": "vite build",
    "build:webcomponents": "cross-env BUILD_TYPE=webcomponents vite build",
    "build:webcomponents_bundle": "cross-env BUILD_TYPE=webcomponents_bundle vite build",
    "typecheck": "vue-tsc --noEmit",
    "preversion": "standard-version && exit 0",
    "release": "standard-version"
  },
  "keywords": [
    "video",
    "canvas",
    "editing",
    "web",
    "vue",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@vicons/carbon": "^0.13.0",
    "@vicons/fluent": "^0.13.0",
    "@webav/av-canvas": "^1.2.7",
    "@webav/av-cliper": "^1.2.7",
    "audiobuffer-to-wav": "^1.0.0",
    "browser-md5-file": "^1.1.1",
    "fods": "^2.3.0",
    "fods-vue": "^2.3.0",
    "indb": "^4.0.1",
    "naive-ui": "^2.38.0",
    "opfs-tools": "^0.7.4",
    "ts-fns": "^13.1.3",
    "vue3-draggable": "^2.0.9"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/wicg-file-system-access": "^2023.10.7",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/test-utils": "^2.4.4",
    "cross-env": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "jsdom": "^24.0.0",
    "less": "^4.4.2",
    "standard-version": "^9.5.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.2",
    "vitest": "^1.2.2",
    "vue": "^3.3.4",
    "vue-tsc": "^3.1.7"
  }
}
